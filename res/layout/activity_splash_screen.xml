<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashScreenVM" type="com.eamtrackerapp.app.modules.splashscreen.data.viewmodel.SplashScreenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSplashScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStackellipseone"
            android:layout_gravity="bottom|start"
            android:layout_height="@dimen/_359pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginTop="@dimen/_20pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageEllipseOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_359pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_ellipse1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1"/>
            <LinearLayout android:id="@+id/linearColumneam"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_101pxv"
                android:layout_marginEnd="@dimen/_120pxh"
                android:layout_marginStart="@dimen/_120pxh"
                android:layout_marginTop="@dimen/_101pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtEAM"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{splashScreenVM.splashScreenModel.txtEAM}"
                    style="@style/txt" tools:text="@string/lbl_eam"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTracker"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{splashScreenVM.splashScreenModel.txtTracker}"
                    style="@style/txt_1" tools:text="@string/lbl_tracker"/>
            </LinearLayout>
        </FrameLayout>
        <ImageView android:id="@+id/imageImageOne"
            android:layout_gravity="top|end"
            android:layout_height="@dimen/_132pxh"
            android:layout_marginBottom="@dimen/_244pxv"
            android:layout_marginEnd="@dimen/_72pxh"
            android:layout_marginStart="@dimen/_72pxh"
            android:layout_marginTop="@dimen/_244pxv"
            android:layout_width="@dimen/_108pxh"
            android:scaleType="fitXY" imageRes="@{@drawable/img_image1}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image1"/>
    </FrameLayout>
</layout>
